{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "slug": "section-2",
    "title": "Style 2",
    "blockTypes": [
        "core/group",
        "core/columns",
        "core/column"
    ],
    "styles": {
       "color": {
            "background": "var(--wp--preset--color--theme-4)",
            "text": "var(--wp--preset--color--theme-5)"
        },
        "blocks": {
            "core/separator": {
                "color": {
                    "text": "currentColor"
                }
            }
        },
        "elements": {
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--theme-5)"
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--theme-5)",
                    "text": "var(--wp--preset--color--theme-1)"
                }
            }
        }
    }
}